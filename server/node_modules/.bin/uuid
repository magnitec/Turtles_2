#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/uuid@7.0.3/node_modules/uuid/dist/bin/uuid/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/uuid@7.0.3/node_modules/uuid/dist/bin/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/uuid@7.0.3/node_modules/uuid/dist/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/uuid@7.0.3/node_modules/uuid/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/uuid@7.0.3/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules:/mnt/c/Users/mgnit/Desktop/node_modules:/mnt/c/Users/mgnit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/uuid/dist/bin/uuid/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/uuid/dist/bin/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/uuid/dist/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/uuid/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/bin/uuid" "$@"
else 
  exec node  "$basedir/../uuid/dist/bin/uuid" "$@"
fi
