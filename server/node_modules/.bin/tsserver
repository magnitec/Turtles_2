#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/typescript@3.9.7/node_modules/typescript/bin/tsserver/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/typescript@3.9.7/node_modules/typescript/bin/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/typescript@3.9.7/node_modules/typescript/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/typescript@3.9.7/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules/.pnpm/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/node_modules:/mnt/c/Users/mgnit/Desktop/node_modules:/mnt/c/Users/mgnit/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/typescript/bin/tsserver/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/typescript/bin/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules/typescript/node_modules:/mnt/c/Users/mgnit/Desktop/Turtles_2/server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
